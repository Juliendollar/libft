#include <stdio.h>

void *ft_memcpy(void *dest, const void *src, size_t n)
{
    size_t i;
    char *destination;
    const char *source;

    destination = (char *)dest;
    source = (const char *)src;
    i = 0;

    while (i < n)
    {
        destination[i] = source[i];
        i++;
    }
    destination[i] = 0;
    return(dest);
}

int main(void)
{
    char dest[10];
    char src[] = "abcde";
    printf("%s", ft_memcpy(dest, src, 3));
}
