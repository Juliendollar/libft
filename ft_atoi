#include <stddef.h>
#include "libft.h"

int ft_atoi(const char *str)
{
    int signe;
    int i;
    int res;

    i = 0;
    res = 0;
    signe = 1;
    while ((str[i] >= 9 && str[i] <= 13) || str[i] == 32)
        i++;
    if (str[i] == '-' || str[i] == '+')
    {
        if (str[i] == '-')
            signe = - 1;
        i++;
    }
    while (str[i] >= '0' && str[i] <= '9')
    {
        res = res * 10 + str[i] - 48;
        i++;
    }
    return (res * signe);
}
